{
  "name": "api-spec-converter",
  "version": "2.12.0",
  "engines": {
    "node": ">=6.0.0"
  },
  "description": "Convert API descriptions between popular formats such as OpenAPI(fka Swagger), RAML, API Blueprint, WADL, etc.",
  "main": "index.js",
  "scripts": {
    "browserify": "node node_modules/mkdirp/bin/cmd dist && browserify --transform-key=somethingNotUsed -t [ babelify --presets [ @babel/preset-env ] ] -s APISpecConverter -o dist/api-spec-converter.js .",
    "test": "mocha && npm run browserify && karma start --single-run && git checkout -- dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lucybot/api-spec-converter.git"
  },
  "keywords": [
    "API",
    "REST",
    "Restful",
    "convert",
    "converter",
    "OpenAPI",
    "OpenAPI Specification",
    "Swagger",
    "RAML",
    "Blueprint",
    "API Blueprint",
    "GoogleDiscovery",
    "WADL",
    "I/O Docs"
  ],
  "contributors": [
    "Bobby Brennan <bobby@lucybot.com> (https://lucybot.com)",
    "Ivan Goncharov <ivan.goncharov.ua@gmail.com> (http://apis.guru)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lucybot/api-spec-converter/issues"
  },
  "homepage": "https://github.com/lucybot/api-spec-converter",
  "bin": {
    "api-spec-converter": "./bin/api-spec-converter"
  },
  "dependencies": {
    "bluebird": "^3.7.2",
    "commander": "^2.20.3",
    "composite-error": "^0.1.1",
    "deep-sort-object": "^1.0.2",
    "js-yaml": "^3.14.0",
    "json-schema-faker": "0.5.0-rcv.46",
    "lodash": "^4.17.21",
    "lodash.clonedeep": "^4.5.0",
    "request": "^2.88.2",
    "source-map": "^0.6.0",
    "sway": "^2.0.6",
    "traverse": "^0.6.6",
    "urijs": "^1.19.11",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.24.4",
    "core-js": "^3.37.0",
    "@babel/preset-env": "^7.24.4",
    "babelify": "^10.0.0",
    "browserify": "^17.0.0",
    "chai": "^3.5.0",
    "karma": "^6.4.3",
    "karma-chai-plugins": "^0.9.0",
    "karma-chrome-launcher": "^3.2.0",
    "karma-mocha": "^2.0.1",
    "karma-mocha-reporter": "^2.2.5",
    "mkdirp": "^0.5.5",
    "mocha": "^10.4.0",
    "puppeteer": "^22.0.0"
  },
  "optionalDependencies": {
    "apib2swagger": "1.9.0",
    "drafter.js": "^2.6.7",
    "google-discovery-to-swagger": "2.0.0",
    "raml-parser": "^0.8.18",
    "raml-to-swagger": "^1.1.0",
    "swagger-converter": "^1.5.1",
    "swagger2openapi": "2.9.*"
  }
}
